        <body>
            <mdiv>
                <parts>
                    <part label="Soprano">
                        <scoreDef meter.count="2" meter.unit="4" key.sig="0">
                            <staffGrp>
                                <label>Soprano</label>
                                <staffDef n="1" lines="5" clef.line="2" clef.shape="G"/>
                            </staffGrp>
                        </scoreDef>    
                        <section>
                            <pb facs="#page_id_102"/> <!-- page beginning -->
                            <sb facs="#region_id_3030"/> <!-- system beginning -->
                            <measure n="1">
                                <staff n="1">
                                    <layer n="1">
                                        <note pname="c" oct="4" dur="1" facs="#symbol_id_5001"/> <!-- agnostic symbol link -->
                                    </layer>
                                </staff>
                            </measure>
                            <!-- other measures -->
                            <sb facs="#region_id_3031"/>
                            <measure n="5">
                                <staff n="1">
                                    <layer n="1">
                                        <note pname="d" oct="4" dur="1"/>
                                    </layer>
                                </staff>
                            </measure>           
                            <!-- more measures -->
                            <pb facs="#page_id_103"/>
                            <sb facs="#region_id_3033"/>
                            <measure n="1">
                                <staff n="1">
                                    <layer n="1">
                                        <note pname="e" oct="4" dur="1"/>
                                    </layer>
                                </staff>
                            </measure>                            
                        </section>
                    </part>
                    <part label="Piano">
                        <scoreDef meter.count="2" meter.unit="4" key.sig="0">
                            <staffGrp symbol="brace">
                                <label>Piano</label>
                                <staffDef n="1" lines="5" clef.line="2" clef.shape="G"/>
                                <staffDef n="2" lines="5" clef.line="4" clef.shape="F"/>
                            </staffGrp>
                        </scoreDef>            
                        <section>
                            <pb facs="#page_id_110"/> <!-- page beginning -->
                            <sb facs="#region_id_4001"/> <!-- system beginning -->
                            <measure n="1">
                                <staff n="1">
                                    <layer n="1">
                                        <note pname="c" oct="4" dur="1"/>
                                    </layer>
                                </staff>
                                <staff n="2">
                                    <layer n="1">
                                        <note pname="e" oct="4" dur="1"/>
                                    </layer>
                                </staff>
                            </measure>
                            <!-- other measures -->   
