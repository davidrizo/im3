    <music>  
        <facsimile>
            <!-- the image304.jpg" contains two pages -->
            <surface xml:id="page_id_102" ulx="0" uly="0" lrx="500" lry="900">
                <graphic target="image304.jpg"></graphic> <!-- TODO If there are several graphics, how do we know which graphic is each zone related to? -->
                <zone xml:id="region_id_3030" ulx="20" uly="30" lrx="203" lry="119" type="region"></zone>
                <zone xml:id="region_id_4001" ulx="1" uly="4" lrx="1000" lry="150" type="agnostic_symbol"></zone>                                                        
                <zone xml:id="region_id_3031" ulx="22" uly="121" lrx="201" lry="319" type="region"></zone>
                <zone xml:id="region_id_3032" ulx="21" uly="339" lrx="200" lry="499"  type="region"></zone>
                <zone xml:id="symbol_id_5001" ulx="30" uly="33" lrx="60" lry="90"  type="region"></zone>
                <!-- other regions -->
                <!-- agnostic symbols could also be defined here as zones -->
            </surface>            
            <surface xml:id="page_id_103" ulx="500" uly="0" lrx="1100" lry="900">
                <graphic target="image304.jpg"></graphic>
                <zone xml:id="region_id_3033" ulx="23" uly="31" lrx="201" lry="131" type="region"></zone>
                   <!-- define more regions here -->
            </surface>
            <surface xml:id="page_id_110" ulx="0" uly="0" lrx="1100" lry="900">
